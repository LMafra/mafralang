set copy_set(set s) {

    ans  EMPTY;
    elem x;
    forall(x  s) add(ans);
    return ans;
}

set succ(set n) {
    set next;
    next = copy_set(n);
    return add(n in next);
}

int main() {
    set zero; set set two; set three;
   
    zero = EMPTY;
    one = succ zero);
    two = succ(one);
}